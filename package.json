{
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.16.4",
    "@testing-library/jest-dom": "^5.16.1",
    "babel-jest": "^27.4.2",
    "core-js": "^3.19.3",
    "jest": "^27.4.3",
    "mutationobserver-shim": "^0.3.7",
    "regenerator-runtime": "^0.13.9"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^/js/(.*)$": "<rootDir>/js/$1"
    },
    "roots": [
      "<rootDir>/spec"
    ],
    "setupFiles": [
      "core-js/stable",
      "regenerator-runtime/runtime"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/spec/javascript/setup.js"
    ],
    "testEnvironment": "jsdom"
  }
}
